{
    "id": "detections-contrail",
    "name": "Contrails",
    "type": "inference-timeseries",
    "s3_location": "detections-contrail",
    "is_periodic": false,
    "time_unit": "day",
    "source": {
        "type": "geojson",
        "tiles": [
            "{api_url}/detections/plane/{spotlightId}/{date}.geojson"
        ]
    },
    "background_source": {
        "type": "raster",
        "tiles": [
            "https://gibs.earthdata.nasa.gov/wmts/epsg4326/best/MODIS_Terra_CorrectedReflectance_TrueColor/default/{date}/250m/{z}/{y}/{x}.jpeg"
        ]
    },
    "exclusive_with": [
        "agriculture",
        "no2",
        "co2-diff",
        "co2",
        "gibs-population",
        "car-count",
        "nightlights-viirs",
        "nightlights-hd",
        "detection-multi",
        "water-chlorophyll",
        "water-spm",
        "detections-ship"
    ],
    "enabled": false,
    "swatch": {
        "color": "#C0C0C0",
        "name": "Grey"
    },
    "info": ""
}